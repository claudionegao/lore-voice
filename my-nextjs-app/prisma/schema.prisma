// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int     @id @default(autoincrement())
  nome    String
  skill   String
  host    Boolean @default(false)
  agoraId String? // nullable
}
